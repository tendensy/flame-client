<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#000000">

    <com.tuyenmonkey.mkloader.MKLoader
        android:id="@+id/MKLoader"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|center"
        app:layout_constraintBottom_toTopOf="@+id/textView5555"
        app:layout_constraintEnd_toEndOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:mk_color="#ffffff"
        app:mk_type="FishSpinner" />

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/back_placeint"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="5dp" />

    <ProgressBar
        android:id="@id/progressBarnew0"
        style="?android:progressBarStyleHorizontal"
        android:layout_width="321dp"
        android:layout_height="16dp"
        android:layout_marginBottom="15dp"
        android:indeterminate="false"
        android:max="100"
        android:progress="0"
        android:progressDrawable="@drawable/progressbar_download"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/textView7777"
        app:layout_constraintEnd_toEndOf="@+id/imageView24"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/imageView24" />

    <ProgressBar
        android:id="@+id/progressBarInstallda0"
        style="?android:progressBarStyleHorizontal"
        android:layout_width="326dp"
        android:layout_height="18dp"
        android:indeterminate="false"
        android:max="100"
        android:minWidth="200.0dip"
        android:minHeight="50.0dip"
        android:padding="0.0dip"
        android:progress="0"
        android:progressDrawable="@drawable/progressbar_download"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@+id/progressBarnew0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView24"
        app:layout_constraintTop_toTopOf="@+id/progressBarnew0" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="0.0dip"
        android:layout_height="0.0dip"
        android:background="@drawable/back"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="153dp"
        android:layout_height="148dp"
        android:layout_marginTop="72dp"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_placeint" />

    <TextView
        android:id="@+id/textView5555"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:fontFamily="@font/pt_root_ui_medium"
        android:text="Загрузка файлов игры..."
        android:textColor="#ffcacaca"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/progressBarnew0"
        app:layout_constraintEnd_toEndOf="@+id/progressBarnew0"
        app:layout_constraintStart_toStartOf="@+id/progressBarnew0" />

    <TextView
        android:id="@+id/textView6666"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pt_root_ui_medium"
        android:text="0"
        android:textColor="#ffffffff"
        android:textSize="14.0sp"
        app:layout_constraintBottom_toBottomOf="@+id/MKLoader"
        app:layout_constraintEnd_toEndOf="@+id/MKLoader"
        app:layout_constraintStart_toStartOf="@+id/MKLoader"
        app:layout_constraintTop_toTopOf="@+id/MKLoader" />

    <TextView
        android:id="@+id/textView7777"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="150dp"
        android:fontFamily="@font/pt_root_ui_medium"
        android:text="0 MB из 0 MB"
        android:textColor="#ffcacaca"
        android:textSize="15.0sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView24"
        app:layout_constraintEnd_toEndOf="@+id/imageView24"
        app:layout_constraintStart_toStartOf="@+id/imageView24" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montsserat_semibold"
        android:gravity="center"
        android:text="PLACEINT"
        android:textColor="@color/white"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

</androidx.constraintlayout.widget.ConstraintLayout>